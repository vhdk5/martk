              "url": "https://video.udacity-data.com/topher/2019/January/5c367437_image1/image1.png",
              "non_google_url": "https://s3.cn-north-1.amazonaws.com.cn/u-img/a8073609-fdb5-4e9c-adf8-e68cabea4464",
