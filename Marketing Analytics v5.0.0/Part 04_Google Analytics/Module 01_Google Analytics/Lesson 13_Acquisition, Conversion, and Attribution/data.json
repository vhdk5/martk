            {
              "id": 809790,
              "key": "7fcd3cff-c4d2-4778-b2de-dd7221cd547b",
              "title": null,
              "semantic_type": "ImageAtom",
              "is_public": true,
              "url": "https://video.udacity-data.com/topher/2019/January/5c367498_image9/image9.png",
              "non_google_url": "https://s3.cn-north-1.amazonaws.com.cn/u-img/7fcd3cff-c4d2-4778-b2de-dd7221cd547b",
              "caption": "",
              "alt": "",
              "width": 701,
