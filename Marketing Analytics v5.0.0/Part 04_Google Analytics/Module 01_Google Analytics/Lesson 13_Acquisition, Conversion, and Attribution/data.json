            {
              "id": 809786,
              "key": "72638a26-a4ce-4bb0-9a5f-ff5164724f7c",
              "title": null,
              "semantic_type": "TextAtom",
              "is_public": true,
              "text": "In Firebase Console you can easily create dynamic links for your mobile app campaigns:\n",
