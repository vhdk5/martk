        {
          "id": 751683,
          "key": "e3da0cec-2992-4b41-894c-625a6f38ecfc",
          "title": "A few more things to consider",
          "semantic_type": "Concept",
          "is_public": true,
          "user_state": {
            "node_key": "e3da0cec-2992-4b41-894c-625a6f38ecfc",
            "completed_at": null,
            "last_viewed_at": null,
