        {
          "id": 751773,
          "key": "24a192ea-e105-4eeb-b02b-6db614cc8d1a",
          "title": "Metrics Definitions",
          "semantic_type": "Concept",
          "is_public": true,
          "user_state": {
            "node_key": "24a192ea-e105-4eeb-b02b-6db614cc8d1a",
            "completed_at": null,
            "last_viewed_at": null,
