            {
              "id": 440381,
              "key": "82ae1cf8-98d5-4906-9c20-845948d480a0",
              "title": null,
              "semantic_type": "TextAtom",
              "is_public": true,
              "text": "## How Do I Get To My Career Portal?\nTo access the Career Portal, look for the briefcase icon in your left-hand sidebar when you're logged into your [Udacity classroom](https://classroom.udacity.com/me).",
              "instructor_notes": ""
